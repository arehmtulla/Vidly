@model Vidly.Models.Customer
@{
    ViewBag.Title = "Customer";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Customer</h2>

<h2>@Model.Name</h2>

<ul>
    <li>
        @Model.MembershipType.Name
    </li>

        @if (!String.IsNullOrEmpty(Model.Birthdate))
        {
            <li>@Model.Birthdate</li>
        }
</ul>